  <li class="table-view-cell text-center "><a href="/weixin/dealers/<%=@dealer.id%>/current_user/mending/orders">预约订单</a></li>
  <%end%>
 
  <li class="table-view-cell text-center "><a href="/weixin/dealers/<%=@dealer.id%>/current_user/bulk_purchasing/orders">团购订单</a></li>
  
   
