<% if @source %>
  <% if @parent.klass == Tips::BulkPurchasing2Order %>
    <%= render "index_common2" %>
  <% else %>
    <%= render "index_common" %>
  <% end %>
<% else %>
  <% case params[:type] %>
  <% when "mending" %>
    <%= render "index_mending" %>
  <% when "bulk_purchasing" %>
    <%= render "index_bulk_purchasing" %>
  <% when "vip_card" %>
    <%= render "index_vip_card" %>
  <% end %>
<% end %>
