<div class="page-header">
  <h3><%= @bulk_purchasing2.title %> <small></small></h3>
</div>

<div class="row-fluid">
  <div class="span3">
    <img src="<%= @bulk_purchasing2.image.url(:medium) %>">
  </div>
  <div class="span9">
    <%= show_for @bulk_purchasing2 do |s| %>
      <%= s.attribute :bulk_purchasing_type %>
      <%= s.attribute :expire_at %>
      <%= s.attribute :vip_price %>
      <%= s.attribute :price %>
      <%= s.attribute :inventory %>
      <%= s.attribute :description %>
    <% end %>
    <%= link_to "预订", new_dashboard_bulk_purchasing2_bulk_purchasing2_order_path(@bulk_purchasing2), class: 'pull-right btn btn-primary' %>
  </div>
</div>

<h3>供货商信息：</h3>

<%= render 'dashboard/distributors/info', distributor: @bulk_purchasing2.distributor %>