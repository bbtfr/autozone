<div class="row-fluid">
  <div class="span3">
    <%= render "posts/share/sidenav/admin" %>
  </div>
  
  <div class="span9">
    <div class="page-header">
      <h3>服务商管理 <small><%#= link_to "添加", new_accounts_dealer_path %></small></h3>
    </div>

    <%= index_for @posts, class: :datatable do |t| %>
      <%= t.attribute :id %>
      <%= t.attribute :username, in: :user %>
      <%= t.attribute :content %>
      <%= t.attribute :image do |image| %>
        <%= image_tag(image.url(:thumb)) %>
      <% end %>
      <%= t.actions :destroy %>
    <% end %>

  </div>
</div>