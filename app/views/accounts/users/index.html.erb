<div class="row-fluid">
  <div class="span3">
    <%= render "accounts/share/sidenav/admin" %>
  </div>
  
  <div class="span9">
    <div class="page-header">
      <h3>车主管理 <small><%#= link_to "添加", new_accounts_dealer_path %></small></h3>
    </div>

    <% @users = @users.page(params[:page]) %>

    <%= index_for @users, class: 'table' do |t| %>
      <%= t.attribute :id %>
      <%= t.attribute :avatar do |avatar| %>
        <%= image_tag(avatar.url(:thumb)) %>
      <% end %>
      <%= t.attribute :username %>
      <%= t.actions :destroy %>
    <% end %>

    <%= paginate @users %>

  </div>
</div>