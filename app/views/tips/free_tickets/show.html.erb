<div class="row-fluid">
  <div class="span3">
    <%= render "tips/share/sidenav/#{@user_type}" %>
  </div>
  
  <div class="span9">
    <div class="page-header">
      <h3><%= @free_ticket.title %> <small><a href="<%= edit_tips_free_ticket_path(@free_ticket) %>">编辑</a></small></h3>
    </div>

    <div class="row-fluid">
      <div class="span3">
        <img src="<%= @free_ticket.image.url(:medium) %>">
      </div>
      <div class="span9">
        <%= show_for @free_ticket do |s| %>
          <%= s.attribute :ticket_type %>
          <%= s.attribute :expire_at %>
          <%= s.attribute :vip_price %>
          <%= s.attribute :price %>
          <%= s.attribute :description %>
        <% end %>
      </div>
    </div>
  </div>

</div>
